/* pages/mine/mine.wxss */
page {
    background-color: #99ce7a;
}
.head {
    display: flex;
    background-color: #fff;
}
.portrait {
    margin: 20rpx 40rpx;
}
.container {
    margin-top: 20rpx;
}
p {
    display: block;
}
.title1 {
    font-size: 1.5em;
}
.title2 {
    font-size: 0.8em;
    color: #a2a2a2;
}
.action {
    background-color: #fff;
    margin-top: 0.6em;
    padding: 1.5em 0.3em;
    display: flex;
    justify-content: space-between;
}
.public_box {
    margin: 0.6em 0;
    background-color: #fff;
    /* 设置为BFC容器 */
    overflow: hidden;  
}
.filled {
    width: 100%;
    height: 100%;
}
.shop {
    padding: 66rpx 40rpx;
    background-color: #fff;
}
.shop_head {
    display: flex;
    align-items: center;
    margin-bottom: 66rpx;
}
.shop_head_image {
    width: 34rpx;
    height: 34rpx;
    margin-right: 1em;
}
.shop_image_box {
    width: 100%;
    height: 376rpx;
}
.title4 {
    margin: 26rpx 0;
    font-size: 0.8em;
}
.title5 {
    font-size: 0.8em;
    color: #a2a2a2;
    margin-bottom: 80rpx;
}


